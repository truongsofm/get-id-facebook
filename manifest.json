{
  "name": "Get Facebook ID",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "Get Facebook ID for Facebook pages.",
  "browser_action": {
    "default_name": "FBID",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "*://*.facebook.com/*"]
}
